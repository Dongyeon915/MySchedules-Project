<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.my_schedules.dao.CalendarDAO">
  <!-- 달력을 위한 유저의 모든 정보를 얻어오기 -->
  <select id="getCalendarUserId" parameterType="TodoResultDTO" resultType="TodoResultDTO">
    SELECT * FROM todo_result where user_id = #{userId}
  </select>

  <select id="allCalendarUserId" parameterType="TodoResultDTO" resultType="TodoResultDTO">
    SELECT * FROM todo_result where user_id = #{userId}
  </select>


</mapper>